# Create a factor from a random sample of LETTERS
set.seed(123)  # Setting seed for reproducibility
letters_factor <- factor(sample(LETTERS, 100, replace = TRUE))

# Display the original levels of the factor
print("Original Levels:")
print(levels(letters_factor))

# Extract five random levels from the factor
sampled_levels <- sample(levels(letters_factor), 5)

# Display the sampled levels
print("\nSampled Levels:")
print(sampled_levels)
