numeric_vector <- c(1, 2, 3, 4, 5)
character_vector <- c("apple", "banana", "cherry", "date", "elderberry")
logical_vector <- c(TRUE, FALSE, TRUE, TRUE, FALSE)
cat("Numeric Vector:\n")
print(numeric_vector)
cat("Data Type:", class(numeric_vector), "\n\n")

cat("Character Vector:\n")
print(character_vector)
cat("Data Type:", class(character_vector), "\n\n")

cat("Logical Vector:\n")
print(logical_vector)
cat("Data Type:", class(logical_vector), "\n")
