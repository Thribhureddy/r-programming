first_10_lowercase <- letters[1:10]
last_10_uppercase <- LETTERS[17:26]
between_22nd_and_24th_uppercase <- LETTERS[22:24]
cat("First 10 English letters in lowercase: ", first_10_lowercase, "\n")
cat("Last 10 letters in uppercase: ", last_10_uppercase, "\n")
cat("Letters between 22nd to 24th in uppercase: ", between_22nd_and_24th_uppercase, "\n")
